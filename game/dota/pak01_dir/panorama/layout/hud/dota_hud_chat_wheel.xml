<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/hudstyles.vcss_c" />
		<include src="s2r://panorama/styles/hud/dota_hud_chat_wheel.vcss_c" />
	</styles>
	<DOTAChatWheel class="Hidden" hittest="false">
		<Panel id="Wheel">
			<Panel id="Arrow0" class="Arrow Hidden" />
			<Panel id="Arrow1" class="Arrow Hidden" />
			<Panel id="Arrow2" class="Arrow Hidden" />
			<Panel id="Arrow3" class="Arrow Hidden" />
			<Panel id="Arrow4" class="Arrow Hidden" />
			<Panel id="Arrow5" class="Arrow Hidden" />
			<Panel id="Arrow6" class="Arrow Hidden" />
			<Panel id="Arrow7" class="Arrow Hidden" />
			<Panel id="PointerContainer">
				<Panel id="WheelPointer" />
			</Panel>
			<Panel id="WheelBG">
				<Panel id="WheelCenter">
					<Panel id="WheelBG2">

					</Panel>
				</Panel>
			</Panel>
			<Panel id="Bubble" />
		</Panel>
		<Label id="Phrase0" class="Phrase" text="{s:phrase1}" />
		<Label id="Phrase1" class="Phrase" text="{s:phrase2}" />
		<Label id="Phrase2" class="Phrase" text="{s:phrase3}" />
		<Label id="Phrase3" class="Phrase" text="{s:phrase4}" />
		<Label id="Phrase4" class="Phrase" text="{s:phrase5}" />
		<Label id="Phrase5" class="Phrase" text="{s:phrase6}" />
		<Label id="Phrase6" class="Phrase" text="{s:phrase7}" />
		<Label id="Phrase7" class="Phrase" text="{s:phrase8}" />
	</DOTAChatWheel>
</root>



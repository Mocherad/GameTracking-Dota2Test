<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/battle_pass/current_battle_pass.vcss_c" />
		<include src="s2r://panorama/styles/ui_change_log_entry.vcss_c" />
	</styles>

	<DOTAChangeLogEntry class="ChangeLogEntry" >
		<Panel class="TypeIcon"/>
		<Panel class="LogContainer">
			<Label class="Description" text="{s:description}" html="true"/>
			<Panel class="BottomLine" >
				<Panel class="TimeContainer LeftRightFlow">
					<Panel class="TimeIcon"/>
					<Label class="Date" text="{t:s:timestamp}"/>
				</Panel>
				<!--<Label class="ButtonBevel ViewLog" text="#DOTA_ChangeLog_View" />-->
				<Label class="ButtonBevel ViewLog" />
			</Panel>
		</Panel>
	</DOTAChangeLogEntry>
</root>

